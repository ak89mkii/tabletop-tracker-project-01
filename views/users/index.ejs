<%- include('../partials/header') %>

<body>
  <div class="loginBox">
  <div class="navbar-fixed">
      <div class="nav-wrapper">
        <ul class="right">
          <li>
          <a href="" class="logText">Users</a>
          <li>
          <% if (user) { %>
            <a class="logText" href="/auth/logout">Log Out</a>
          <% } else { %>
            <a class="logText" href="/auth/google">Login with Google</a>
          <% } %>
          </li>
        </ul>
      </div>
  </div>
  <ul>Registered Users:
    <% users.forEach(u => { %>
      <li><%= u.name %></li>
    <% }) %>
  </ul>
</div>

<%- include('../partials/footer') %>